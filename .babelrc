{
  "plugins": [
    "external-helpers",
    "transform-function-bind",
    ["transform-async-to-module-method", {"module": "bluebird", "method": "coroutine"}],
    ["transform-runtime", {"polyfill": false, "regenerator": true}]
  ],
  "presets": ["es2015"],
  "env": {
    "test_coverage": {
      "plugins": [
        ["istanbul", {"exclude": ["test/*.js", "**/*_spec.js"]}]
      ]
    }
  }
}
